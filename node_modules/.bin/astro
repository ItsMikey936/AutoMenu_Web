#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/ruin0/Documents/Proyectos Web/AutoMenu_Web/node_modules/.pnpm/astro@5.12.9_@types+node@24_57dedc3838ed2877a0f6376b8e069a13/node_modules/astro/node_modules:/mnt/c/Users/ruin0/Documents/Proyectos Web/AutoMenu_Web/node_modules/.pnpm/astro@5.12.9_@types+node@24_57dedc3838ed2877a0f6376b8e069a13/node_modules:/mnt/c/Users/ruin0/Documents/Proyectos Web/AutoMenu_Web/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/ruin0/Documents/Proyectos Web/AutoMenu_Web/node_modules/.pnpm/astro@5.12.9_@types+node@24_57dedc3838ed2877a0f6376b8e069a13/node_modules/astro/node_modules:/mnt/c/Users/ruin0/Documents/Proyectos Web/AutoMenu_Web/node_modules/.pnpm/astro@5.12.9_@types+node@24_57dedc3838ed2877a0f6376b8e069a13/node_modules:/mnt/c/Users/ruin0/Documents/Proyectos Web/AutoMenu_Web/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/astro@5.12.9_@types+node@24_57dedc3838ed2877a0f6376b8e069a13/node_modules/astro/astro.js" "$@"
else
  exec node  "$basedir/../.pnpm/astro@5.12.9_@types+node@24_57dedc3838ed2877a0f6376b8e069a13/node_modules/astro/astro.js" "$@"
fi
