-- CREACIÓN DE TABLAS --

CREATE TABLE Meals_Time (
    mTime_ID INTEGER PRIMARY KEY AUTOINCREMENT,
    mTime_Name VARCHAR(30),
    mTime: VARCHAR (20)
);

CREATE TABLE Meals (
    meal_ID INTEGER PRIMARY KEY AUTOINCREMENT,
    mTime_ID INTEGER,
    meal_Name VARCHAR(50),
    meal_Ingredients VARCHAR(10),
    porcion_Ingrediente REAL,
    FOREIGN KEY (id_Tipo_Ingrediente) REFERENCES Tipos_Ingredientes(id_Tipo_Ingrediente)
);


