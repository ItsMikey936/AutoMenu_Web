-- CREACIÓN DE TABLAS --

CREATE TABLE Dias (
    id_Dias INTEGER PRIMARY KEY AUTOINCREMENT,
    dia VARCHAR(20)
);

CREATE TABLE Tipos_Comida (
    id_Tipo_Comida INTEGER PRIMARY KEY AUTOINCREMENT,
    nombre_Tipo VARCHAR(30)
);

CREATE TABLE Tipos_Ingredientes (
    id_Tipo_Ingrediente INTEGER PRIMARY KEY AUTOINCREMENT,
    nombre_Tipo_Ingrediente VARCHAR(30)
);

CREATE TABLE Ingredientes (
    id_Ingrediente INTEGER PRIMARY KEY AUTOINCREMENT,
    id_Tipo_Ingrediente INTEGER,
    nombre_Ingrediente VARCHAR(50),
    unidad_Ingrediente VARCHAR(10),
    porcion_Ingrediente REAL,
    FOREIGN KEY (id_Tipo_Ingrediente) REFERENCES Tipos_Ingredientes(id_Tipo_Ingrediente)
);

CREATE TABLE Menus (
    id_Menu INTEGER PRIMARY KEY AUTOINCREMENT,
    id_Dias INTEGER,
    id_Tipo_Comida INTEGER,
    FOREIGN KEY (id_Dias) REFERENCES Dias(id_Dias),
    FOREIGN KEY (id_Tipo_Comida) REFERENCES Tipos_Comida(id_Tipo_Comida)
);

CREATE TABLE Menus_Ingredientes (
    id_Menus_Ingredientes INTEGER PRIMARY KEY AUTOINCREMENT,
    id_Menu INTEGER,
    id_Ingrediente INTEGER,
    cantidad_Porcion INTEGER,
    FOREIGN KEY (id_Menu) REFERENCES Menus(id_Menu),
    FOREIGN KEY (id_Ingrediente) REFERENCES Ingredientes(id_Ingrediente)
);


